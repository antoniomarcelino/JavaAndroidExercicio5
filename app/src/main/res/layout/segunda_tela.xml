<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <TextView
        android:id="@+id/dadosCadastrados"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textSize="30dp"
        android:fontFamily="sans-serif-black"
        android:text="Dados Cadastrados"
        android:textColor="#000000"
        android:layout_marginTop="15dp"
        android:textAlignment="center"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/textoNome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Nome Completo"
        android:layout_below="@id/editar"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="#333939"
        ></TextView>

    <TextView
        android:id="@+id/nomeCadastrado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Nome Cadastrado"
        android:layout_below="@id/textoNome"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="@color/teal_700"
        ></TextView>

    <TextView
        android:id="@+id/textoCpf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Cadastro de Pessoa FÃ­sica"
        android:layout_below="@id/nomeCadastrado"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="#333939"
        ></TextView>

    <TextView
        android:id="@+id/cpfCadastrado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="777.888.999-44"
        android:layout_below="@id/textoCpf"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="@color/teal_700"
        ></TextView>

    <TextView
        android:id="@+id/textoDataNascimento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Data de Nascimento"
        android:layout_below="@id/cpfCadastrado"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="#333939"
        ></TextView>

    <TextView
        android:id="@+id/dataNascimentoCadastrado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="01/01/2001"
        android:layout_below="@id/textoDataNascimento"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="@color/teal_700"
        ></TextView>

    <TextView
        android:id="@+id/textoTelefone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Telefone"
        android:layout_below="@id/dataNascimentoCadastrado"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="#333939"
        ></TextView>

    <TextView
        android:id="@+id/telefoneCadastrado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="(62)32823282)"
        android:layout_below="@id/textoTelefone"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="@color/teal_700"
        ></TextView>

    <TextView
        android:id="@+id/textoCelular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Celular"
        android:layout_below="@id/telefoneCadastrado"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="#333939"
        ></TextView>

    <TextView
        android:id="@+id/celularCadastrado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="(62)981819191)"
        android:layout_below="@id/textoCelular"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="@color/teal_700"
        ></TextView>

    <TextView
        android:id="@+id/textoEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Email"
        android:layout_below="@id/celularCadastrado"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="#333939"
        ></TextView>

    <TextView
        android:id="@+id/emailCadastrado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="endereco@email.com"
        android:layout_below="@id/textoEmail"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:textSize="14dp"
        android:textColor="@color/teal_700"
        ></TextView>

    <Button
        android:id="@+id/editar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dadosCadastrados"
        android:backgroundTint="#009688"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="14dp"
        android:layout_centerHorizontal="true"
        android:text="Editar"></Button>

    <Button
        android:id="@+id/voltar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/emailCadastrado"
        android:backgroundTint="#009688"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="10dp"
        android:layout_centerHorizontal="true"
        android:text="Voltar"></Button>



</RelativeLayout>